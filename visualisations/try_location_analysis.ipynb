   "source": [
    "## Configuration"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Configuration\n",
    "YEARS_TO_ANALYZE = [2024, 2025, 2026]\n",
    "TOTAL_ROUNDS = 27\n",
    "\n",
    "# Field zone definitions (based on try timing patterns)\n",
    "FIELD_ZONES = {\n",
    "    'Left Wing': {'minutes': [0, 20, 40, 60], 'pattern': 'early_left'},\n",
    "    'Left Center': {'minutes': [10, 30, 50, 70], 'pattern': 'mid_left'},\n",
    "    'Through Middle': {'minutes': [15, 35, 55, 75], 'pattern': 'center'},\n",
    "    'Right Center': {'minutes': [20, 40, 60, 80], 'pattern': 'mid_right'},\n",
    "    'Right Wing': {'minutes': [25, 45, 65, 85], 'pattern': 'late_right'}\n",
    "}\n",
    "\n",
    "# Time periods\n",
    "TIME_PERIODS = {\n",
    "    'Early (0-20)': (0, 20),\n",
    "    'Mid (21-40)': (21, 40),\n",
    "    'Mid (41-60)': (41, 60),\n",
    "    'Late (61-80)': (61, 80)\n",
    "}\n",
    "\n",
    "logger.info(f\"Configuration loaded: {YEARS_TO_ANALYZE} years\")"
   ]
