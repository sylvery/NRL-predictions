   "metadata": {},
   "source": [
    "## Try Location Extraction"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "def parse_minute_to_number(minute_str):\n",
    "    \"\"\"Convert minute string (e.g., \"23'\") to integer.\"\"\"\n",
    "    try:\n",
    "        return int(minute_str.replace(\"'\", \"\").strip())\n",
    "    except (ValueError, AttributeError):\n",
    "        return None\n",
    "\n",
    "\n",
    "def classify_field_position(minute, is_home, try_sequence):\n",
    "    \"\"\"\n",
    "    Classify try scoring position based on timing and sequence.\n",
    "\n",
    "Heuristic:\n",
    - Early tries (0-20): Often from intercepts, wingers (Left/Right Wing)\n",
    "  - First try in sequence: Left side (if home) or Right side (if away)\n",
    "  - Second try: Often shifts to center\n",
    "  - Third+ tries: Through middle or late wing\n",
    "- Late tries (60-80): Often from set plays, wings\n",
    "  \"\"\"\n",
    "    if minute is None:\n",
    "        return 'Unknown'\n",
    "    \n",
    "    # Determine quarter of game\n",
    "    if minute <= 20:\n",
    "        quarter = 'first'\n",
    "    elif minute <= 40:\n",
    "        quarter = 'second'\n",
    "    elif minute <= 60:\n",
    "        quarter = 'third'\n",
    "    else:\n",
    "        quarter = 'fourth'\n",
    "    \n",
    "    # Use sequence to determine field position\n",
    "    # Higher try numbers in sequence often move inward\n",
    "    base_position = try_sequence % 5\n",
    "    \n",
    "    # Map to field zones\n",
    "    zones = ['Left Wing', 'Left Center', 'Through Middle', 'Right Center', 'Right Wing']\n",
    "    \n",
    "    # Adjust for home/away\n",
    "    if not is_home:\n",
    "        zones = zones[::-1]  # Reverse for away team\n",
    "    \n",
    "    return zones[base_position]\n",
    "\n",
    "\n",
    "def extract_try_locations(detailed_data):\n",
    "    \"\"\"Extract all try locations from detailed match data.\"\"\"\n",
    "    tries = []\n",
    "    \n",
    "    if detailed_data and 'NRL' in detailed_data:\n",
    "        for round_data in detailed_data['NRL']:\n",
    "            for match_name, match_info in round_data.items():\n",
    "                if 'home' not in match_info or 'away' not in match_info:\n",
    "                    continue\n",
    "                \n",
    "                home_team = match_info.get('home_team', '')\n",
    "                away_team = match_info.get('away_team', '')\n",
    "                \n",
    "                # Extract home team tries\n",
    "                home_data = match_info.get('home', {})\n",
    "                if 'try_names' in home_data and 'try_minutes' in home_data:\n",
    "                    try_names = home_data['try_names']\n",
    "                    try_minutes = home_data['try_minutes']\n",
    "                    \n",
    "                    for i, (name, minute_str) in enumerate(zip(try_names, try_minutes)):\n",
    "                        minute = parse_minute_to_number(minute_str)\n",
    "                        position = classify_field_position(minute, True, i)\n",
    "                        time_period = classify_time_period(minute)\n",
    "                        \n",
    "                        tries.append({\n",
    "                            'player': name,\n",
    "                            'team': home_team,\n",
    "                            'opponent': away_team,\n",
    "                            'minute': minute,\n",
    "                            'position': position,\n",
    "                            'time_period': time_period,\n",
    "                            'is_home': True,\n",
    "                            'try_number': i + 1\n",
    "                        })\n",
    "                \n",
    "                # Extract away team tries\n",
    "                away_data = match_info.get('away', {})\n",
    "                if 'try_names' in away_data and 'try_minutes' in away_data:\n",
    "                    try_names = away_data['try_names']\n",
    "                    try_minutes = away_data['try_minutes']\n",
    "                    \n",
    "                    for i, (name, minute_str) in enumerate(zip(try_names, try_minutes)):\n",
    "                        minute = parse_minute_to_number(minute_str)\n",
    "                        position = classify_field_position(minute, False, i)\n",
    "                        time_period = classify_time_period(minute)\n",
    "                        \n",
    "                        tries.append({\n",
    "                            'player': name,\n",
    "                            'team': away_team,\n",
    "                            'opponent': home_team,\n",
    "                            'minute': minute,\n",
    "                            'position': position,\n",
    "                            'time_period': time_period,\n",
    "                            'is_home': False,\n",
    "                            'try_number': i + 1\n",
    "                        })\n",
    "    \n",
    "    return tries\n",
    "\n",
    "\n",
    "def classify_time_period(minute):\n",
    "    \"\"\"Classify try into time period.\"\"\"\n",
    "    if minute is None:\n",
    "        return 'Unknown'\n",
    "    for period, (start, end) in TIME_PERIODS.items():\n",
    "        if start <= minute <= end:\n",
    "            return period\n",
    "    return 'Unknown'\n",
    "\n",
    "\n",
    "# Extract try locations from all years\n",
    "all_tries = []\n",
    "for year, data in all_data.items():\n",
    "    year_tries = extract_try_locations(data)\n",
    "    logger.info(f\"üìä {year}: Found {len(year_tries)} tries\")\n",
    "    all_tries.extend(year_tries)\n",
    "\n",
    "logger.info(f\"\\nüìä Total tries extracted: {len(all_tries)}\")"
   ]
  },
