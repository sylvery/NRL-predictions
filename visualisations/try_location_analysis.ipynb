    "def create_nrl_field():\n",
    "    \"\"\"Create NRL field visualization.\"\"\"\n",
    "    fig = go.Figure()\n",
    "    \n",
    "    # Field outline\n",
    "    fig.add_shape(\n",
    "        type=\"rect\", x0=0, y0=0, x1=100, y1=68,\n",
    "        line=dict(color=\"white\", width=2),\n",
    "        fillcolor=\"green\"\n",
    "    )\n",
    "    \n",
    "    # Try lines (40m from each end)\n",
    "    fig.add_shape(type=\"line\", x0=40, y0=0, x1=40, y1=68, line=dict(color=\"white\", width=2))\n",
    "    fig.add_shape(type=\"line\", x0=60, y0=0, x1=60, y1=68, line=dict(color=\"white\", width=2))\n",
    "    \n",
    "    # Center line\n",
    "    fig.add_shape(type=\"line\", x0=50, y0=0, x1=50, y1=68, line=dict(color=\"white\", width=2))\n",
    "    \n",
    "    # Center circle\n",
    "    fig.add_shape(\n",
    "        type=\"circle\", x0=45, y0=31, x1=55, y1=37,\n",
    "        line=dict(color=\"white\", width=2)\n",
    "    )\n",
    "    \n",
    "    # In-goal areas\n",
    "    fig.add_shape(type=\"rect\", x0=0, y0=0, x1=10, y1=68, line=dict(color=\"white\", width=2), fillcolor=\"lightgreen\")\n",
    "    fig.add_shape(type=\"rect\", x0=90, y0=0, x1=100, y1=68, line=dict(color=\"white\", width=2), fillcolor=\"lightgreen\")\n",
    "    \n",
    "    return fig\n",
    "\n",
    "\n",
    "def map_position_to_coordinates(position):\n",
    "    \"\"\"Map field position to x,y coordinates on the field.\"\"\"\n",
    "    positions = {\n",
    "        'Left Wing': (5, 60),\n",
    "        'Left Center': (25, 60),\n",
    "        'Through Middle': (50, 34),\n",
    "        'Right Center': (75, 60),\n",
    "        'Right Wing': (95, 60),\n",
    "    }\n",
    "    return positions.get(position, (50, 34))\n",
    "\n",
    "\n",
    "# Create field visualization\n",
    "fig = create_nrl_field()\n",
    "\n",
    "# Add try locations as scatter plot\n",
    "x_coords = []\n",
    "y_coords = []\n",
    "colors = []\n",
    "\n",
    "for _, try_info in df.iterrows():\n",
    "    x, y = map_position_to_coordinates(try_info['position'])\n",
    "    # Add jitter for visibility\n",
    "    x += np.random.uniform(-5, 5)\n",
    "    y += np.random.uniform(-10, 10)\n",
    "    x_coords.append(x)\n",
    "    y_coords.append(y)\n",
    "    colors.append(try_info['time_period'])\n",
    "\n",
    "fig.add_trace(go.Scatter(\n",
    "    x=x_coords, y=y_coords,\n",
    "    mode='markers',\n",
    "    marker=dict(\n",
    "        size=10,\n",
    "        color=colors,\n",
    "        colorscale='Viridis',\n",
    "        showscale=True\n",
    "    ),\n",
    "    text=[f\"{row['player']} ({row['minute']}')\" for _, row in df.iterrows()],\n",
    "    hoverinfo='text'\n",
    "))\n",
    "\n",
    "fig.update_layout(\n",
    "    title=\"NRL Try Locations by Time Period\",\n",
    "    xaxis_title=\"Field Width\",\n",
    "    yaxis_title=\"Field Length\",\n",
    "    showlegend=True,\n",
    "    template=\"plotly_dark\",\n",
    "    height=600,\n",
    "    width=800\n",
    ")\n",
    "\n",
    "fig.show()"
   ]
